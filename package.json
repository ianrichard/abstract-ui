{
    "name": "abstract-ui",
    "version": "0.0.0",
    "description": "Exploration if a configuration-driven approach to generating UI is a viable approach to creating scalable experiences.",
    "scripts": {
        "clean": "shx rm -rf build",
        "webpack": "webpack --config config/webpack.config.production.js",
        "sass": "node-sass src/styles/index.scss build/index.css --output-style compressed",
        "build": "npm run clean && npm run sass && npm run webpack",
        "lint": "eslint src config",
        "dev": "webpack-cli serve --mode development --config config/webpack.config.dev.js",
        "start": "serve -s build"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ianrichard/abstract-ui"
    },
    "author": "Ian Smith",
    "license": "MIT",
    "homepage": "https://github.com/ianrichard/abstract-ui",
    "dependencies": {
        "@babel/core": "^7.12.3",
        "@babel/polyfill": "^7.12.1",
        "@babel/preset-env": "^7.12.1",
        "@babel/preset-react": "^7.12.5",
        "autoprefixer": "^10.0.2",
        "babel-loader": "^8.2.0",
        "babel-plugin-module-resolver": "^4.0.0",
        "copy-webpack-plugin": "^6.3.0",
        "css-loader": "^5.0.1",
        "file-loader": "^5.1.0",
        "history": "^4.10.1",
        "html-webpack-plugin": "^4.5.0",
        "node-sass": "^5.0.0",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-router-dom": "^5.2.0",
        "sass-loader": "^10.0.5",
        "serve": "^11.3.2",
        "shx": "^0.3.3",
        "style-loader": "^2.0.0",
        "webpack": "^5.4.0",
        "webpack-cli": "^4.2.0",
        "webpack-dev-server": "^3.11.0"
    },
    "devDependencies": {
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-config-standard": "^14.1.1",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-react": "^7.21.5",
        "eslint-plugin-standard": "^4.1.0",
        "prettier": "2.6.1"
    }
}

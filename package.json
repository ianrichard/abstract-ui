        "@emotion/react": "^11.8.2",
        "@emotion/styled": "^11.8.1",
        "@mui/icons-material": "^5.8.4",
        "@mui/material": "^5.8.4",
        "@mui/styled-engine-sc": "^5.8.0",
        "autoprefixer": "^10.4.7",
        "babel-loader": "^8.2.5",
        "babel-plugin-module-resolver": "^4.0.0",
